#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/mime@2.5.2/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules:/Users/evstorm/Desktop/node_modules:/Users/evstorm/node_modules:/Users/node_modules:/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/superagent@6.1.0/node_modules/superagent/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/superagent@6.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/mime@2.5.2/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules:/Users/evstorm/Desktop/node_modules:/Users/evstorm/node_modules:/Users/node_modules:/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/superagent@6.1.0/node_modules/superagent/node_modules:/Users/evstorm/Desktop/getUniapp-doc/node_modules/.pnpm/superagent@6.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
fi
